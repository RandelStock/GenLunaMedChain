{
  "name": "hardhat-javascript-starter",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "npx thirdweb@latest detect",
    "deploy": "npx thirdweb@latest deploy",
    "publish": "npx thirdweb@latest publish",
    "start": "node server.js",
    "dev": "nodemon server.js",
    "deploy:testnet": "node scripts/deploy-testnet.js",
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:seed": "npx prisma db seed",
    "db:reset": "npx prisma migrate reset --force",
    "db:migrate": "npx prisma migrate deploy",
    "db:reset:neon": "node prisma/reset-neon.js",
    "db:nuke": "node prisma/reset-neon.js && npm run seed:all",
    "seed:all": "node prisma/seed-all.js",
    "seed:main": "node prisma/seed.js",
    "seed:residents": "node prisma/seed-resident.js",
    "seed:barangay": "node prisma/seed-barangay.js",
    "seed:suppliers": "node prisma/seed-suppliers.js",
    "seed:releases": "node prisma/seed-medicine-releases.js",
    "seed:removals": "node prisma/seed-stock-removals.js",
    "seed:consultations": "node prisma/seed-consultations.js",
    "seed:calendar": "node prisma/seed-calendar-events.js",
    "seed:providers": "node prisma/seed-provider-data.js",
    "verify:residents": "node prisma/verify-residents.js",
    "db:fresh": "npm run db:reset && npm run seed:all"
  },
  "devDependencies": {
    "@matterlabs/hardhat-zksync-solc": "^1.1.14",
    "@matterlabs/hardhat-zksync-verify": "^1.7.1",
    "@nomicfoundation/hardhat-toolbox": "^6.1.0",
    "hardhat": "^2.26.3",
    "nodemon": "^3.1.10",
    "prisma": "^6.16.3"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^5.3.0",
    "@prisma/client": "^6.16.3",
    "@sendgrid/mail": "^8.1.6",
    "@thirdweb-dev/contracts": "^3.8.0",
    "body-parser": "^2.2.0",
    "chart.js": "^4.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "ethers": "^6.7.0",
    "express": "^5.1.0",
    "node-cache": "^5.1.2",
    "nodemailer": "^7.0.9",
    "react-chartjs-2": "^5.3.0",
    "zksync-ethers": "^5.7.0"
  },
  "prisma": {
    "seed": "node prisma/seed-all.js"
  }
}